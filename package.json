{
  "name": "mr-poke",
  "version": "1.0.0",
  "description": "",
  "keywords": [],
  "license": "MIT",
  "author": "m03geek",
  "main": "index.js",
  "scripts": {
    "build": "pnpm run build:node && pnpm run build:copy",
    "build:clean": "rm -rf dist",
    "build:copy": "exit 0",
    "build:node": "tsc -p tsconfig.prod.json",
    "lint": "pnpm run lint:types && pnpm run lint:codestyle && pnpm run lint:packages",
    "lint:codestyle": "eslint . --ext js,jsx,ts,tsx",
    "lint:packages": "pnpm audit --prod --audit-level=high",
    "lint:types": "tsc -p tsconfig.json",
    "migrate:create": "prisma migrate dev --create-only --name",
    "migrate:development": "prisma migrate dev",
    "migrate:production": "prisma migrate deploy",
    "migrate:reset": "prisma migrate reset",
    "pino:pretty": "pino-pretty -i pid,hostname -t 'SYS:standard'",
    "poc": "tsx src/playground.ts",
    "start": "pnpm run build && node dist/bin/server.js",
    "start:dev": "tsx watch --inspect src/bin/server.ts | pnpm run pino:pretty",
    "test": "pnpm run build && pnpm run lint && pnpm run test:unit",
    "test:unit": "jest --passWithNoTests"
  },
  "dependencies": {
    "@fastify/autoload": "^5.7.1",
    "@fastify/cookie": "^9.1.0",
    "@fastify/error": "^3.4.0",
    "@fastify/oauth2": "^7.4.0",
    "@gitbeaker/rest": "^39.16.0",
    "@slack/web-api": "^6.9.0",
    "dotenv": "^16.3.1",
    "fastify": "^4.23.2",
    "fastify-plugin": "^4.5.1",
    "http-status-codes": "^2.3.0",
    "luxon": "^3.4.3",
    "pino": "^8.15.3",
    "source-map-support": "^0.5.21",
    "ts-app-env": "^1.5.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.1.0",
    "@tsconfig/node-lts": "^18.12.5",
    "@tsconfig/strictest": "^2.0.2",
    "@types/luxon": "^3.3.2",
    "@types/node": "^20.6.3",
    "pino-pretty": "^10.2.0",
    "prettier": "^3.0.3",
    "prettier-plugin-packagejson": "^2.4.6",
    "prettier-plugin-prisma": "^5.0.0",
    "tsx": "^3.13.0",
    "typescript": "^5.2.2"
  }
}
